{
  "name": "starter",
  "version": "12.2.8",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200",
    "build": "ng lint && ng test --watch=false --code-coverage && ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --watch=false --code-coverage",
    "lint": "ng lint",
    "doc": "npx compodoc -s -p tsconfig.doc.json",
    "analyse": "ng build --stats-json; npx webpack-bundle-analyzer dist/starter/stats.json",
    "doc:gen": "./node_modules/.bin/compodoc -p tsconfig.doc.json"
  },
  "author": {
    "name": "Terrence Miao",
    "email": "info@paradise.org",
    "url": "https://www.paradise.org/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.8",
    "@angular/common": "12.2.8",
    "@angular/compiler": "12.2.8",
    "@angular/core": "12.2.8",
    "@angular/forms": "12.2.8",
    "@angular/platform-browser": "12.2.8",
    "@angular/platform-browser-dynamic": "12.2.8",
    "@angular/router": "12.2.8",
    "rxjs": "7.3.1",
    "tslib": "2.3.1",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.8",
    "@angular-eslint/builder": "12.5.0",
    "@angular-eslint/eslint-plugin": "12.5.0",
    "@angular-eslint/eslint-plugin-template": "12.5.0",
    "@angular-eslint/schematics": "12.5.0",
    "@angular-eslint/template-parser": "12.5.0",
    "@angular/cli": "12.2.8",
    "@angular/compiler-cli": "12.2.8",
    "@compodoc/compodoc": "^1.1.15",
    "@types/jasmine": "3.9.1",
    "@types/node": "16.10.2",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "^7.26.0",
    "jasmine-core": "3.9.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "sass": "^1.42.1",
    "typescript": "4.3.5"
  }
}
